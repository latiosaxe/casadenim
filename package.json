{
    "name": "jarvis",
    "version": "3.0.0",
    "description": "Casa Denim",
    "engines": {
        "node": ">=6.9.1"
    },
    "scripts": {
        "start": "gulp --gulpfile config/gulp.js --shopify=true",
        "html": "gulp --gulpfile config/gulp.js --shopify=false",
        "watch": "npm run start",
        "gulp": "npm run start",
        "open": "shopify-themekit open",
        "configure": "shopify-themekit configure -d ../shopify/ --ignored-file config/settings_data.json",
        "download": "shopify-themekit download -d ./shopify/",
        "dev": "cross-env NODE_ENV=development npm run gulp -- dev",
        "build": "cross-env NODE_ENV=build npm run gulp -- build",
        "prod": "cross-env NODE_ENV=production npm run gulp -- build",
        "deploy": "cross-env NODE_ENV=build npm run start -- deploy",
        "deploy-prod": "cross-env NODE_ENV=production npm run start -- deploy",
        "purge": "npm run start -- purge",
        "clean": "npm run start -- purge"
    },
    "dependencies": {
        "aos": "^2.3.4",
        "choozy": "0.0.3",
        "globby": "^8.0.2",
        "jquery": "^3.4.1",
        "lazim": "1.0.0",
        "mitt": "^1.1.3",
        "operator": "^1.6.1",
        "picoapp": "^3.5.0",
        "shopify-api-node": "^2.18.1",
        "sliced": "^1.0.1",
        "slick-carousel": "^1.8.1",
        "svbstrate": "^4.1.1",
        "unfetch": "^4.1.0"
    },
    "devDependencies": {
        "@shopify/themekit": "^0.6.12",
        "autoprefixer": "^7.2.6",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.5",
        "babel-preset-env": "^1.7.0",
        "babel-preset-stage-2": "^6.22.0",
        "browser-sync": "^2.26.3",
        "cross-env": "^5.2.0",
        "cssnano": "^3.10.0",
        "del": "^3.0.0",
        "expose-loader": "^0.7.5",
        "fs-extra": "^5.0.0",
        "gulp": "4.0",
        "gulp-if": "^2.0.2",
        "gulp-imagemin": "^4.1.0",
        "gulp-notify": "^3.2.0",
        "gulp-plumber": "^1.2.1",
        "gulp-postcss": "^6.4.0",
        "gulp-posthtml": "^3.0.4",
        "gulp-rename": "^1.4.0",
        "gulp-sass": "^3.2.1",
        "gulp-sourcemaps": "^2.6.5",
        "imagemin-mozjpeg": "^6.0.0",
        "imagemin-pngquant": "^5.1.0",
        "js-yaml": "^3.12.2",
        "md5-file": "^3.2.3",
        "npm-run-all": "^4.1.5",
        "postcss": "^6.0.23",
        "postcss-easy-import": "^3.0.0",
        "postcss-flexbugs-fixes": "^3.3.1",
        "postcss-lazy-rules": "github:2createstudio/postcss-lazy-rules",
        "postcss-loader": "^2.1.6",
        "postcss-sprites": "^4.2.0",
        "postcss-url": "^7.3.2",
        "postcss-utilities": "^0.7.0",
        "posthtml-include": "^1.2.0",
        "slash": "^1.0.0",
        "uglify-js": "^3.4.10",
        "uglifyjs-webpack-plugin": "^1.3.0",
        "webpack": "^3.12.0",
        "webpack-stream": "^4.0.3",
        "yargs": "^10.1.2"
    }
}
